edDeprecatedRules":"32"},{"filePath":"60","messages":"61","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"32"},"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\index.js",[],["62","63"],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\App.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\GlobalState.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\headers\\Header.js",["64"],"import React, {useContext, useState} from 'react'\r\nimport {GlobalState} from '../../GlobalState'\r\nimport Menu from './icon/menu.svg'\r\nimport Close from './icon/close.svg'\r\nimport Cart from './icon/cart.svg'\r\nimport {Link} from 'react-router-dom'\r\nimport axios from 'axios'\r\n\r\nfunction Header() {\r\n    const state = useContext(GlobalState)\r\n    const [isLogged] = state.userAPI.isLogged\r\n    const [isAdmin] = state.userAPI.isAdmin\r\n    const [cart] = state.userAPI.cart\r\n\r\n\r\n    const logoutUser = async () =>{\r\n        await axios.get('/user/logout')\r\n        localStorage.clear()\r\n        window.location.href = \"/\";\r\n    }\r\n\r\n    const adminRouter = () =>{\r\n        return(\r\n            <>\r\n                <li><Link to=\"/create_product\">Create Product</Link></li>\r\n                <li><Link to=\"/category\">Categories</Link></li>\r\n            </>\r\n        )\r\n    }\r\n\r\n    const loggedRouter = () =>{\r\n        return(\r\n            <>\r\n                <li><Link to=\"/history\">History</Link></li>\r\n                <li><Link to=\"/\" onClick={logoutUser}>Logout</Link></li>\r\n            </>\r\n        )\r\n    }\r\n\r\n    return (\r\n        <header>\r\n            <div className=\"menu\" >\r\n                <img src={Menu} alt=\"\" width=\"30\" />\r\n            </div>\r\n\r\n            <div className=\"logo\">\r\n                <h1>\r\n                    <Link to=\"/\">{isAdmin ? 'Admin' : 'Gallery Shop'}</Link>\r\n                </h1>\r\n            </div>\r\n\r\n\r\n            <ul>\r\n                <li><Link to=\"/\">{isAdmin ? 'Products' : 'Shop'}</Link></li>\r\n\r\n                {isAdmin && adminRouter()}\r\n\r\n                {\r\n                    isLogged ? loggedRouter() : <li><Link to=\"/login\">Login or Register</Link></li>\r\n                }\r\n                \r\n                <li>\r\n                    <img src={Close} alt=\"\" width=\"30\" className=\"menu\" />\r\n                </li>\r\n\r\n            </ul>\r\n\r\n            {\r\n                isAdmin ? '' \r\n                :<div className=\"cart-icon\">\r\n                    <span>{cart.length}</span>\r\n                    <Link to=\"/cart\">\r\n                        <img src={Cart} alt=\"\" width=\"30\"/>\r\n                    </Link>\r\n                </div>\r\n            }\r\n\r\n            \r\n        </header>\r\n    )\r\n}\r\n\r\nexport default Header","C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\mainpages\\Pages.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\mainpages\\auth\\Register.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\mainpages\\auth\\Login.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\mainpages\\cart\\Cart.js",["65"],"import React, {useContext, useState, useEffect} from 'react'\r\nimport {GlobalState} from '../../../GlobalState'\r\nimport {Link} from 'react-router-dom'\r\nimport axios from 'axios'\r\n\r\nfunction Cart() {\r\n    const state = useContext(GlobalState)\r\n    const [cart, setCart] = state.userAPI.cart\r\n    const [token] = state.token\r\n    const [total, setTotal] = useState(0)\r\n\r\n    //ovo se stavlja pod useEffect jer kada react rendera nasu komponentu, pamtit ce se effect koji smo koristili i update-at DOM-a\r\n    useEffect(() =>{ \r\n        const getTotal = () =>{\r\n            const total = cart.reduce((prev, item) => {// spremamo u prev(predstavlja akumulator) te nadodajemo novu vrijednost na prev\r\n                return prev + (item.price * item.quantity)//dodavanje na prethodnika odnosno na akumulator\r\n            },0)\r\n\r\n            setTotal(total)//postavi res koji je definiran u arrow funkciji\r\n        }\r\n\r\n        getTotal()//vracanje funkcije\r\n\r\n    },[cart])//drugi argument je niz koji sadrži dependency za usporedbu\r\n\r\n\r\n    const addToCart = async () =>{\r\n        await axios.patch('/user/addcart', {cart}, {\r\n            headers: {Authorization: token}\r\n        })\r\n    }\r\n\r\n\r\n    const increment = (id) =>{\r\n        cart.forEach(item => {\r\n            if(item._id === id){\r\n                item.quantity += 1\r\n            }\r\n        })\r\n\r\n        setCart([...cart])\r\n        addToCart()\r\n    }\r\n\r\n\r\n    const decrement = (id) =>{ //smanjenje count-a, šaljemo id proizvoda kojeg želimo smanjiti\r\n        cart.forEach(item => {//prolazi se loop petljon kroz sve iteme(proizvode) te trazimo odgovarajući i povecavamo count bez da ista vracamo !!\r\n            if(item._id === id){//ako je ovo true, trazi dok ne naides na true\r\n                item.quantity === 1 ? item.quantity = 1 : item.quantity -= 1 //provjera izraza, ako je true(count je 1) onda ne smanjuj,a inače smanji\r\n            }\r\n        })\r\n\r\n        setCart([...cart])// postavi promjenu na kartici\r\n        addToCart()\r\n    }\r\n\r\n\r\n    const removeProduct = id =>{\r\n        if(window.confirm(\"Zelite li izbrisati proizvod?\")){\r\n            cart.forEach((item, index) => {\r\n                if(item._id === id){\r\n                    cart.splice(index, 1)\r\n                }\r\n            })\r\n\r\n            setCart([...cart])\r\n            addToCart()\r\n        }\r\n    }\r\n\r\n\r\n    if(cart.length === 0)\r\n        return <h2 style={{textAlign: \"center\", fontSize: \"5rem\"}}>Kosarica prazna</h2>\r\n    \r\n    return (\r\n        <div>\r\n            {\r\n                cart.map(product => (//za svaki proizvod u kartici\r\n                    <div className=\"detail cart\" key={product._id}>\r\n                        <img src={product.images.url} alt=\"\" height=\"120\" />\r\n                        \r\n                        <div className=\"box-detail\">\r\n                            <h2>{product.title}</h2>\r\n                            <br /> \r\n                            <h3>{product.price * product.quantity} kn</h3>\r\n                            <p>{product.description}</p>\r\n                            <br /> \r\n                            {/*botuni za smanjenje, povecanje i brisanje proizvoda */}\r\n                            <div className=\"amount\">\r\n                                <button onClick={() => decrement(product._id)}> - </button>\r\n                                <span>{product.quantity}</span>\r\n                                <button onClick={() => increment(product._id)}> + </button>\r\n                            </div>\r\n\r\n                            <div className=\"delete\" \r\n                            onClick={() => removeProduct(product._id)}>\r\n                                X\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                ))\r\n            }\r\n            <br /> \r\n            <div className=\"total\">\r\n                <h3>Sveukupno: {total} kn</h3>\r\n                <button onClick={() => alert('Hvala na kupovini!')}>\r\n                    <h2>Platiti</h2>\r\n                </button>\r\n                \r\n                \r\n            </div>\r\n            <br /> \r\n            <br /> \r\n        </div>\r\n    )\r\n}\r\nexport default Cart","C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\mainpages\\products\\Products.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\mainpages\\utils\\not_found\\NotFound.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\api\\ProductsAPI.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\components\\mainpages\\utils\\productItem\\ProductItem.js",[],"C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\api\\UserAPI.js",["66"],"import {useState, useEffect, useContext} from 'react'\r\nimport axios from 'axios'\r\n\r\nfunction  UserAPI(token) {\r\n    const [isLogged, setIsLogged] = useState(false)\r\n    const [isAdmin, setIsAdmin] = useState(false)\r\n    const [cart, setCart] = useState([])\r\n\r\n\r\n    useEffect(() =>{\r\n        if(token){\r\n            const getUser = async () =>{\r\n                try {\r\n                    const res = await axios.get('/user/infor', {\r\n                        headers: {Authorization: token}\r\n                    })\r\n\r\n                    setIsLogged(true)\r\n                    res.data.role === 1 ? setIsAdmin(true) : setIsAdmin(false)\r\n\r\n                    setCart(res.data.cart)\r\n\r\n                } catch (err) {\r\n                    alert(err.response.data.msg)\r\n                }\r\n            }\r\n\r\n            getUser()\r\n            \r\n        }\r\n    },[token])\r\n    \r\n    const addCart = async (product) => {\r\n        //if(!isLogged) return alert(\"Logirajte se prije kupnje\")\r\n\r\n        const check = cart.every(item =>{\r\n            return item._id !== product._id\r\n        })\r\n\r\n        if(check){\r\n            setCart([...cart, {...product, quantity: 1}])\r\n\r\n            await axios.patch('/user/addcart', {cart: [...cart, {...product, quantity: 1}]}, {\r\n                headers: {Authorization: token}\r\n            })\r\n\r\n        }else{\r\n            alert(\"Proizvod je vec dodan u kosaricu.\")\r\n        }\r\n    }\r\n\r\n    return {\r\n        isLogged: [isLogged, setIsLogged],\r\n        isAdmin: [isAdmin, setIsAdmin],\r\n        cart: [cart, setCart],\r\n        addCart: addCart,\r\n        \r\n    }\r\n}\r\n\r\nexport default UserAPI\r\n","C:\\Users\\Josko\\Desktop\\vjezba6\\client\\src\\compone